{
  "name": "bmo",
  "version": "0.1.0",
  "description": "Basic Vue Application with ES2015, Koa.js",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "start:dev": "run-p start:server:dev start:client:dev",
    "start:client:dev": "cd ./packages/node_modules/@bmo/client && yarn serve",
    "start": "node dist/packages/node_modules/@bmo/server",
    "start:local": "yarn build && node dist/packages/node_modules/@bmo/server",
    "start:server:dev": "nodemon --config ./nodemon.json -r ./scripts/babelRegister ./packages/node_modules/@bmo/server",
    "prebuild": "rimraf build",
    "build": "node -r ./scripts/babelRegister.js scripts/build/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "npm run lint && npm test && npm run test:feature",
    "selenium": "node ./scripts/postinstall",
    "test": "jest",
    "postinstall": "lerna bootstrap && npm run link:cli",
    "link:cli":"echo \"linking cli\" && cd ./packages/cli && yarn link"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@lmig/eslint-config-cm": "^1.0.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-watch": "^7.0.0",
    "cheerio": "^1.0.0-rc.1",
    "eslint": "^5.12.0",
    "eslint-config-i-am-meticulous": "^11.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.4.0",
    "eslint-plugin-vue": "^5.2.2",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "globby": "^9.2.0",
    "jest": "^24.8.0",
    "lerna": "^3.16.4",
    "listr": "^0.14.3",
    "nodemon": "^1.19.1",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@hapi/joi": "^15.0.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "mongodb": "^3.2.6",
    "npm-run-all": "^4.1.5",
    "pkg-up": "^3.1.0",
    "uuid": "^3.3.2"
  },
  "jest":{
    "reporters":["default"],
    "collectCoverage":true,
    "collectCoverageFrom":[
      "**/*.js",
      "!**/node_modules/**"
    ],
    "projects": ["<rootDir>/packages/*"]
  }
}
