{
  "name": "bmo",
  "version": "0.1.0",
  "description": "Basic React Application with ES2015, Express.js, and Webpack 4",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "start:dev": "run-p start:server:dev start:client:dev",
    "start:client:dev": "cd ./packages/node_modules/@bmo/client && yarn serve",
    "start": "node dist/packages/node_modules/@bmo/server",
    "start:local": "yarn build && node dist/packages/node_modules/@bmo/server",
    "start:server:dev": "nodemon --config ./nodemon.json -r ./scripts/babelRegister ./packages/node_modules/@bmo/server",
    "prebuild": "rimraf build",
    "build": "node -r ./scripts/babelRegister.js scripts/build/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "npm run lint && npm test && npm run test:feature",
    "selenium": "node ./scripts/postinstall",
    "test": "exit 0",
    "postinstall": "npm run installSubPackages",
    "installSubPackages": "node -r ./scripts/babelRegister ./scripts/installSubpackages.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@lmig/eslint-config-cm": "^1.0.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-watch": "^7.0.0",
    "cheerio": "^1.0.0-rc.1",
    "eslint": "^5.12.0",
    "eslint-config-i-am-meticulous": "^11.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.4.0",
    "eslint-plugin-vue": "^5.2.2",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "globby": "^9.2.0",
    "listr": "^0.14.3",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@hapi/joi": "^15.0.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "http-methods-enum": "^0.1.1",
    "http-status-codes": "^1.3.2",
    "joi": "^14.3.1",
    "joi-to-swagger": "^3.1.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.1.0",
    "koa-pino-logger": "^2.1.3",
    "koa-proxy": "^1.0.0-alpha.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^2.14.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.6",
    "npm-run-all": "^4.1.5",
    "uuid": "^3.3.2"
  }
}
